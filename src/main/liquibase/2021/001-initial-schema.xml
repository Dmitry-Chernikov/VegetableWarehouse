<?xml version="1.0" encoding="UTF-8" ?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.1.xsd"
        logicalFilePath="001-initial-schema.xml">

  <changeSet  id="001"  author="ChDA">

    <createTable  tableName="VegetableBase">
      <column  name="IDProducts"  type="int" autoIncrement="true">
        <constraints  primaryKey="true"  nullable="false" />
      </column>
      <column  name="IDStore"  type="int">
        <constraints  nullable="false"/>
      </column>
      <column  name="IDProduct"  type="int">
        <constraints  nullable="false"/>
      </column>
      <column  name="DateReceipt"  type="date">
         <constraints nullable="false"/>
      </column>
    </createTable>


  </changeSet>
</databaseChangeLog>


